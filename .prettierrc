{
  "printWidth": 80,
  "tabWidth": 2,
  "trailingComma": "all",
  "singleQuote": true,
  "jsxBracketSameLine": true,
  "semi": true,
  "importOrder": [
    "^react$",
    "^([^./](.*))(?<!(@/.*))(?<!(@sadmammoth/.*))$",
    "^@sadmammoth/(.*)$",
    "^(@/(.*))(?<!((.*)\\.styles\\.?(.*)))$",
    "^([./](.*))(?<!((.*)\\.styles\\.?(.*)))$",
    "^(.*)\\.styles\\.?(.*)$"
  ],
  "importOrderSeparation": false,
  "experimentalBabelParserPluginsList": ["jsx", "typescript"]
}
